<% include partials/header %>
<!-- //form1 for adding a new hotel -->
<div class="container">
        
        <div class=" main-login main-center">
            <form class="form-horizontal" id="addR" method="POST" action="/hotel/">
                <div class="form-group">
                    <label for="hotelImage" class="cols-sm-2 control-label" >Give Image Url</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-envelope fa" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="hotelImage"  placeholder="Image Url" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="hotelCity" class="cols-sm-2 control-label" >Enter Hotel City</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-users fa" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="hotelCity"  placeholder="Enter City" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="hotelName" class="cols-sm-2 control-label" >Enter Hotel Name</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="hotelName"  placeholder="Enter Hotel Name" />
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label for="hotelAddress" class="cols-sm-2 control-label" >Enter Hotel Address</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="hotelAddress"  placeholder="Enter Hotel Address" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="hotelContact" class="cols-sm-2 control-label" >Enter Hotel Contact</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="hotelContact" placeholder="Enter Hotel Contact Number" />
                        </div>
                    </div>
                </div>


                <div class="form-group">
                    <label for="latitude" class="cols-sm-2 control-label" >Enter Hotel latitude</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="latitude" placeholder="Enter Hotel Contact latitude" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="longitude" class="cols-sm-2 control-label" >Enter Hotel longitude</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="longitude" placeholder="Enter Hotel Contact longitude" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="bedrooms" class="cols-sm-2 control-label" >Enter The Numbers Of Bedrooms</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="bedrooms" placeholder="Enter The Numbers Of Bedroom" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="bathrooms" class="cols-sm-2 control-label" >Enter The Numbers Of Bathrooms</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="bathrooms" placeholder="Enter The Numbers Of Bathrooms" />
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label for="carSpaces" class="cols-sm-2 control-label" >Is There Any Car Parking Space</label>
                    <div class="cols-sm-10">
                        <div class="input-group">
                            <span class="input-group-addon"><i class="fa fa-lock fa-lg" aria-hidden="true"></i></span>
                            <input type="text" class="form-control" name="carSpaces" placeholder="Enter Car Space For vichele In Number" />
                        </div>
                    </div>
                </div>


                <div class="form-group ">
                    <button type="submit" class="btn btn-primary btn-lg btn-block login-button">Add Hotel</button>
                </div>

            </form>
        </div>
    </div>

</div>



<% include partials/footer %>